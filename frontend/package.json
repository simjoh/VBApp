{
  "name": "brevet-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config",
    "build": "ng build",
    "buildfordocker": "ng build --configuration=docker",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "startserver": "node mocks/server.js",
    "mock:server": "json-server --watch mocks/db.json",
    "start:proxy": "ng serve --proxy-config proxy.conf.json",
    "start:proxy:mock:server": "concurrently --kill-others \"npm run start:proxy\" \"npm run startserver\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.5",
    "@angular/cdk": "^19.1.3",
    "@angular/common": "^19.1.5",
    "@angular/compiler": "^19.1.5",
    "@angular/core": "^19.1.5",
    "@angular/forms": "^19.1.5",
    "@angular/localize": "^19.1.5",
    "@angular/platform-browser": "^19.1.5",
    "@angular/platform-browser-dynamic": "^19.1.5",
    "@angular/router": "^19.1.5",
    "@ng-select/ng-select": "^14.2.0",
    "@primeng/themes": "^19.0.6",
    "ngx-device-detector": "^9.0.0",
    "or": "^0.2.0",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primeng": "^19.0.6",
    "rxjs": "^7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.6",
    "@angular/cli": "^19.1.6",
    "@angular/compiler-cli": "^19.1.5",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "concurrently": "^6.3.0",
    "jasmine-core": "~3.8.0",
    "json-server": "^0.17.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~5.7.3"
  }
}
