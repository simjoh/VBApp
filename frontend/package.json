{
  "name": "brevet-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config",
    "build": "ng build",
    "buildfordocker": "ng build --configuration=docker",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "startserver": "node mocks/server.js",
    "mock:server": "json-server --watch mocks/db.json",
    "start:proxy": "ng serve --proxy-config proxy.conf.json",
    "start:proxy:mock:server": "concurrently --kill-others \"npm run start:proxy\" \"npm run startserver\""
  },
  "private": true,
