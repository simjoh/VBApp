{
  "name": "brevet/api",
  "type": "project",
  "description": "Vasterbottenbrevt api",
  "keywords": [
    "slim-framework"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3 || ^8.0",
    "ext-json": "*",
    "ext-pdo": "*",
    "miladrahimi/php-jwt": "2.*",
    "php-di/php-di": "^6.3",
    "ramsey/uuid": "^4.2",
    "selective/basepath": "^2.1",
    "slim/psr7": "^1.2",
    "slim/slim": "^4.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit --configuration phpunit.xml",
    "test:coverage": "phpunit --configuration phpunit.xml --coverage-clover build/logs/clover.xml --coverage-html build/coverage"
  }
}
